{
  "name": "starfield-mod-loader",
  "author": "Mychal Thompson <mychal.r.thompson@gmail.com>",
  "repository": "https://github.com/lVlyke/starfield-mod-loader",
  "license": "GPL-3.0",
  "version": "0.5.2",
  "main": "electron.js",
  "scripts": {
    "app:build-dist": "npm run build:release && npm run package",
    "app:build-dist:all": "npm run build:release && npm run package:all",
    "app:build-release": "npm run app:build-dist && npm run prepare-packages",
    "app:build-release:all": "npm run app:build-dist:all && npm run prepare-packages",
    "ng": "./node_modules/.bin/ng",
    "build": "node ./scripts/build.js",
    "build:release": "npm run build -- --release",
    "serve": "node ./scripts/serve.js",
    "serve:release": "npm run serve -- --release",
    "package": "node ./scripts/package.js",
    "package:all": "npm run package -- --all",
    "prepare-packages": "node ./scripts/prepare-packages.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "npm run serve",
    "electron-win32-install": "npm remove electron && npm install && export npm_config_platform=win32 && npm install --save-dev electron@^27.0.0 && unset npm_config_platform"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "~16.2.3",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "~16.2.3",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@lithiumjs/angular": "^7.3.0",
    "@lithiumjs/ngx-material-theming": "^0.10.0",
    "@ngxs/store": "^3.8.1",
    "7zip-bin": "^5.2.0",
    "detect-file-encoding-and-language": "^2.4.0",
    "electron-log": "^4.4.8",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-7z": "^3.0.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "which": "^4.0.0",
    "xml2js": "^0.6.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.198",
    "@types/node": "^18.17.19",
    "@types/node-7z": "^2.1.5",
    "@types/xml2js": "^0.4.12",
    "chokidar": "^3.5.3",
    "electron": "^27.0.0",
    "electron-packager": "^17.1.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "license-checker-rseidelsohn": "^4.2.7",
    "typescript": "~5.1.3"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
